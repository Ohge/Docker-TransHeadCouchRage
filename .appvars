### APP VARS
TYPE=svc
NAME=thcr
CONFIG_DIR=$HOME/.thcr
CONFIG_VOL=/etc/thcr
FETCH_DIR=$HOME/fetch
FETCH_VOL=/fetch
USER_DIR=$HOME/downloads
USER_VOL=/downloads
HTP_PORT=80
SSL_PORT=443
P2P_PORT=54321

### RUNTIME VARS
HOSTNAME=$(hostname)
USER=$(whoami)
IMG=$TYPE/$NAME
APP=$USER.$NAME
LUID=$(id -u $USER)
LGID=$(id -g $USER)
